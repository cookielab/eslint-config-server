{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true,
        "jasmine": true,
        "jest": true
    },
    "plugins": [
        "filenames",
        "@cookielab.io/custom-rules"
    ],
    "extends": [
        "./config/native.js",
        "./config/flow.js",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "rules": {
        "filenames/match-regex": ["error", "^[a-zA-Z0-9]+(\\.[a-zA-Z0-9]+)*$"],

        "@cookielab.io/custom-rules/sort-imports": ["error"]
    }
}
