{
  "name": "@cookielab.io/eslint-config-server",
  "version": "4.0.0",
  "description": "Code Style",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cookielab/eslint-config-server.git"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "peerDependencies": {
    "@cookielab.io/eslint-plugin-custom-rules": "optional:^0.3.0",
    "@typescript-eslint/eslint-plugin": "optional:^2.15.0",
    "@typescript-eslint/parser": "optional:^2.15.0",
    "babel-eslint": "optional:^10.0.3",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "optional:^1.1.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "optional:^4.5.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-sort-imports-es6-autofix": "optional:^0.5.0",
    "typescript": "optional:^3.7.4"
  },
  "devDependencies": {
    "@cookielab.io/eslint-plugin-custom-rules": "0.3.0",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "ajv": "6.10.2",
    "babel-eslint": "10.0.3",
    "clone": "2.1.2",
    "eslint": "6.8.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.5.0",
    "jest": "24.9.0",
    "typescript": "3.7.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.(ts|js)"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  }
}
